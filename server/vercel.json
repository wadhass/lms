{
    "version": 2,
    "build": [
        {
            "src": "verver.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dist": "verver.js"
        }
    ]
}